<body class="container">
<main>
        <div class="jumbotron">
            <h3 class="text-center">Upload Item</h3>
            <form action="/seller/<%=sellerId%>/upload" method="POST" enctype="multipart/form-data">
                <div class="mb-3">
                  <label for="itemName" class="form-label">Name Item</label>
                  <input type="text" class="form-control" id="itemName" name="itemName" placeholder="Name Item">
                </div>
                
                <div class="mb-3">
                    <label for="itemPrice" class="form-label">Price</label>
                    <input type="number" class="form-control" id="itemPrice" name="itemPrice" placeholder="Price">
                  </div>

                  <div class="mb-3">
                    <label for="itemWeight" class="form-label">Weight</label>
                    <input type="number" class="form-control" id="itemWeight" name="itemWeight" placeholder="Weight">
                  </div>

                  <div class="mb-3">
                    <label for="itemQuantity" class="form-label">Stock</label>
                    <input type="number" class="form-control" id="itemQuantity" name="itemQuantity" placeholder="Stock">
                  </div>

                  <div class="mb-3">
                    <label for="desc" class="form-label">Description</label>
                    <input type="text" class="form-control" id="desc" name="desc" placeholder="Description">
                  </div>

                  <div class="mb-3">
                    <label for="itemPictures" class="form-label">Upload item images</label>
                    <input class="form-control" type="file" id="itemPictures" name="files" multiple>
                </div>

                    <div class="text-center">
                        <button type="submit" class="btn btn-success">Submit</button>
                        <a href="/" class="btn btn-danger" aria-current="page">Cancel</a>
                    </div>
              </form>
        </div>
</main>

